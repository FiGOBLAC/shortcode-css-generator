	"gradient": {

		"elements": {
			"all": " .gradient"
		},

		"restrictions": {},

		"declarations": {

			"horizontal": {
				"background": "#7474BF ; /* fallback for old browsers */",
				"background": "-webkit-linear-gradient(to left, {gradient-left} , {gradient-right}); /* Chrome 10-25, Safari 5.1-6 */",
				"background": "linear-gradient(to left,  {gradient-left} , {gradient-right} ; /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */"
			},

			"steller": {
				"background": "#7474BF; /* fallback for old browsers */",
				"background": "-webkit-linear-gradient(to left, #7474BF , #348AC7); /* Chrome 10-25, Safari 5.1-6 */",
				"background": "linear-gradient(to left, #7474BF , #348AC7); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */"
			},

			"maroon": {
				"background": "rgba(204,31,115,1)",
				"background": "-webkit-linear-gradient(top, rgba(204,31,115,1) 0%, rgba(133,20,75,1) 100%)",
				"background": "linear-gradient(to right, rgb(204, 31, 64) 0%, rgb(80, 12, 45) 100%)"
			},

			"center": {
				"border": "1px solid transparent",
				"-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
				"-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
				"-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
				"-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
				"border-image-slice": 1
			},

			"left": {
				"border": "1px solid transparent",
				"-webkit-border-image": "-webkit-linear-gradient(left, {gradient-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
				"-o-border-image": "-o-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
				"-moz-border-image": "-moz-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0))",
				"-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
				"border-image-slice": 1
			},

			"right": {
				"border": "1px solid transparent",
				"-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
				"-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
				"-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
				"-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
				"border-image-slice": 1
			}
		}
	}
}
