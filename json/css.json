    "gradient-sample": {

        "elements": {
            "all": ".my-shortcode-class"
        },

        "restrictions": {
        },

        "declarations": {

            "center": {
                "border": "1px solid transparent",
                "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-sample-color}, rgba(0, 0, 0, 0))",
                "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-sample-color}, rgba(0, 0, 0, 0))",
                "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-sample-color}, rgba(0, 0, 0, 0))",
                "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-sample-color}, rgba(0, 0, 0, 0))",
                "border-image-slice": 1
            },
            "left": {
                "border": "1px solid transparent",
                "-webkit-border-image": "-webkit-linear-gradient(left, {gradient-sample-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
                "-o-border-image": "-o-linear-gradient(left, {gradient-sample-color}, rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
                "-moz-border-image": "-moz-linear-gradient(left, {gradient-sample-color}, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0))",
                "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-sample-color}, rgba(0, 0, 0, 0))",
                "border-image-slice": 1
            },
            "right": {
                "border": "1px solid transparent",
                "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-sample-color})",
                "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-sample-color})",
                "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-sample-color}))",
                "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-sample-color}))",
                "border-image-slice": 1
            }
        }
    }
