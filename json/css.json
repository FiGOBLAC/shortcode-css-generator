{
    "shortcode-option": {
        "type": "custom",
        "shortcode:configs": "change-color:declarations",
        "mediabox:configs": "grandient-color:selectors",
        "panelbox:selector": " .panel-default > .panel-heading, $ .panel-collapse > .panel-body",
        "imagebox:selector": " .panel-default > .panel-heading, $ .panel-collapse > .panel-body",
        "gradient::selector": " .panel-default > .transition, $ .panel-collapse > .panel-body",
        "transitions::selector": " .panel-default > .transition, $ .panel-collapse > .panel-body",
        "animations::selector": " selectors->headerbox",
        "[shortcode]:transitions:selector": "MY NEW CUSTOM SELECTION",
        "[headerbox]:animations:selector": " .panel-default > .transition, $ .panel-collapse > .imagebox",
        "panelbox:declaration": "border: 1px solid transparent",
        "imagebox:declaration": "background:-webkit-linear-gradient(to left, {gradient-left})",
        "headerbox:declaration": "background:-webkit-linear-gradient(to left, {gradient-left})",
        "transitions:filter": ["left", "right"],
        "transitions::filter": ["left", "right"],
        "animations:filter": ["left", "right"],
        "animations::filter": ["left", "right"],
        "[shortcode]:transitions:restrict": ["horizontal", "vertical"],
        "[headerbox]::transitions:restrict": ["horizontal", "vertical"],
        "[headerbox]::declaration:restrict": ["transitions", "animations"],
        "animations:resolve": "left-animation->left",
        "transitions:alias": "left-transition->left"
    },

