{

    "shortcode-height:height":" .myheight $.my-id",
    "background-color":" .mybackground $.my-id",
    "margin-left":" .mybackground $.my-id",

    "shortcode-margin": {
        "type": "custom:native",
        "properties": "margin-left",
        "selector": "selectors->selectbox",
        "selector2": "selectors->selectbox2",
        "left:selector": "selector-value-overrides->valueover",
        "up:selector": "selector-value-overrides->valueover",
        "[shortcode]:left:selector": "selector-shortcode-overrides->shortover"
    },

    "shortcode-margin2": {
        "type": "custom:native",
        "properties": ["padding-top", "padding-bottom"],
        "selector": "selectors->selectbox",
        "left:selector": "selector-value-overrides->valueover",
        "up:selector": "selector-value-overrides->valueover",
        "[shortcode]:left:selector": "selector-shortcode-overrides->shortover"
    },

    "shortcode-option": {
        "type": "custom",
        "gradient:selector": "selectors->selectbox",
        "transitions:selector": "selectors->selectbox",
        "gradient:left:selector": "selector-value-overrides->valueover",
        "gradient:up:selector": "selector-value-overrides->valueover",
        "[shortcode]:gradient:selector": "selector-shortcode-overrides->shortover",
        "[shortcode]:gradient:left:selector": " .my-test-selector",
        "gradient:left:declaration": "-webkit-linear-gradient(left, {gradient-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
        "[shortcode]:gradient:left:declaration": "border: 1px solid transparent-trans",
        "gradient:filter": ["left", "right"]
    },

    "shortcode-option2": {
        "type": "custom",
        "gradient:selector": "selectors->selectbox",
        "gradient:selector2": "selectors->selectbox",
        "gradient:selector3": "selectors->selectbox",
        "transitions:selector": "selectors->selectbox",
        "gradient:left:selector": "selector-value-overrides->valueover",
        "gradient:up:selector": "selector-value-overrides->valueover",
        "[shortcode]:gradient:selector": "selector-shortcode-overrides->shortover",
        "[shortcode]:gradient:left:selector": " .my-test-selector",

        "gradient:filter": ["left", "right"]
    }
}
