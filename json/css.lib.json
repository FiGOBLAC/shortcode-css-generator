{

    "selectors": {
        "headerbox": " .headerbox-lib",
        "panelbox": " .panelbox-lib",
        "imagebox": " imagebox-lib",
        "container": " container-lib"
    },

    "selector-value-override": {
        "headerbox": " .headerbox-lib-value-or",
        "panelbox": " .panelbox-lib-value-or",
        "imagebox": " imagebox-lib-value-or",
        "container": " container-lib-value-or"
    },

    "selector-shortcode-override": {
        "headerbox": " .headerbox-lib-shortcode-or",
        "panelbox": " .panelbox-lib-shortcode-or",
        "imagebox": " imagebox-lib-shortcode-or",
        "container": " container-lib-shortcode-or"
    },


    "gradient": {

        "left": {
            "gradient-left": "gradient-left",
            "border": "1px solid transparent-trans",
            "-webkit-border-image": "-webkit-linear-gradient(left, {gradient-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-o-border-image": "-o-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-moz-border-image": "-moz-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
            "border-image-slice": 1
        },

        "right": {
            "gradient-right": "gradient-right",
            "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "border-image-slice": 1
        },

        "up": {
            "gradient-up": "gradient-up",
            "border": "1px solid transparent",
            "-webkit-border-image": "-webkit-linear-gradient(left, {gradient-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-o-border-image": "-o-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-moz-border-image": "-moz-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
            "border-image-slice": 1
        },

        "down": {
            "gradient-down": "gradient-down",
            "border": "1px solid transparent",
            "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "border-image-slice": 1
        }

    },

    "transitions": {

        "left": {
            "transition-left": "transition-left",
            "background": "1px solid transparent-trans"
        },

        "right": {
            "transitions-right": "transitions-right",
            "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "border-image-slice": 1
        },

        "up": {
            "transitions-up": "transitions-up",
            "border": "1px solid transparent",
            "-webkit-border-image": "-webkit-linear-gradient(left, {gradient-color},rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-o-border-image": "-o-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0),  rgba(0, 0, 0, 0))",
            "-moz-border-image": "-moz-linear-gradient(left, {gradient-color}, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), {gradient-color}, rgba(0, 0, 0, 0))",
            "border-image-slice": 1
        },

        "down": {
            "transitions-down": "transitions-down",
            "border": "1px solid transparent",
            "-webkit-border-image": "-webkit-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-o-border-image": "-o-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color})",
            "-moz-border-image": "-moz-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "-ms-border-image": "-ms-linear-gradient(left, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), {gradient-color}))",
            "border-image-slice": 1
        }
    }
}
